<Window x:Class="ProductPractic4.WorkReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProductPractic4"
        mc:Ignorable="d"
        Title="WorkReportWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Frame Grid.Row="2" x:Name="Frame"></Frame>
        <Grid Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button x:Name="DataSet" Grid.Column="0" Content="DataSet" VerticalAlignment="Center" Click="DataSet_Click"/>
            <Button x:Name="EF" Grid.Column="1" Content="EF" VerticalAlignment="Center" Click="EF_Click"/>
        </Grid>
    </Grid>
</Window>
